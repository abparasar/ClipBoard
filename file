import pandas as pd

# Sample DataFrame
data = {
    'Category': ['A', 'B', 'A', 'B', 'A', 'B'],
    'Value1': [10, 20, 30, 40, 50, 60],
    'Value2': [5, 15, 25, 35, 45, 55]
}
df = pd.DataFrame(data)

# Group by 'Category' and sum 'Value1' and 'Value2'
grouped_df = df.groupby('Category')[['Value1', 'Value2']].sum().reset_index()

print(grouped_df)