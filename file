import pandas as pd

# Define the output Excel file
file_name = "output.xlsx"

# Create an Excel writer object
with pd.ExcelWriter(file_name, engine="xlsxwriter") as writer:
    for i in range(1, 6):  # Example: Loop 5 times to generate DataFrames
        # Generate a sample DataFrame
        df = pd.DataFrame({
            "ID": range(1, 6),
            f"Value_{i}": [x * i for x in range(1, 6)]
        })
        
        # Write DataFrame to a new sheet in the Excel file
        sheet_name = f"Sheet_{i}"  # Naming sheets dynamically
        df.to_excel(writer, sheet_name=sheet_name, index=False)

print(f"Excel file '{file_name}' created successfully with multiple sheets.")