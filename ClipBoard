from sklearn.cluster import KMeans
import numpy as np

# Assume 'data' is your list of 21,000 tensors of 600 dimensions each
data = np.random.rand(21000, 600)  # Replace this with your actual data

# Define the number of clusters (choose 'n_clusters' based on your data)
n_clusters = 10  # Example value

# Initialize and fit the KMeans model
kmeans = KMeans(n_clusters=n_clusters, random_state=42)
kmeans.fit(data)

# Get the cluster labels
labels = kmeans.labels_