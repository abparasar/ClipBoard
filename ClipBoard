import pandas as pd

# Example DataFrame
data = {'RAI Id': [123, 456, 789], 'Name': ['Alice', 'Bob', 'Charlie']}
df = pd.DataFrame(data)

# Add link to 'RAI Id' column
df['RAI Id'] = df['RAI Id'].apply(lambda x: f'<a href="https://www.example.com/{x}">{x}</a>')

# Convert to HTML
html_table = df.to_html(escape=False, index=False)  # escape=False allows HTML tags to render

# Output HTML
print(html_table)