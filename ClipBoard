from scipy.stats import f_oneway
import numpy as np

# Example data: fraud rates (10 bands x 7 months)
fraud_rates = np.random.rand(10, 7) * 0.05  # Replace with actual data

# Perform one-way ANOVA
anova_result = f_oneway(*[fraud_rates[:, i] for i in range(fraud_rates.shape[1])])
print(f"F-Statistic: {anova_result.statistic:.4f}, P-Value: {anova_result.pvalue:.4f}")

if anova_result.pvalue <= 0.05:
    print("Fraud rates differ significantly across months.")
else:
    print("No significant difference in fraud rates across months.")