# Example dictionary mapping keys to values
mapping_dict = {
    "a": 10,
    "b": 20,
    "c": 30,
    "d": 40
}

# List of values
values_list = [10, 30, 50]  # 50 does not exist in the dictionary

# Create a list of keys or keep the value if the value does not exist in the dictionary
result_list = [key for value in values_list if (key := next((k for k, v in mapping_dict.items() if v == value), None)) is not None] + [value for value in values_list if value not in mapping_dict.values()]

# Print the result
print(result_list)