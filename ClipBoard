\documentclass{article}
\usepackage{datatool}

\DTLloaddb{mydata}{data.csv} % Load CSV into memory

\begin{document}

\section*{Filtered Column 2 Values (Where Column 1 = Yes)}

\begin{itemize}
\DTLforeach{mydata}{\colone=col1,\coltwo=col2}{%
  \ifthenelse{\equal{\colone}{Yes}}{%
    \item \coltwo
  }{}
}
\end{itemize}

\end{document}