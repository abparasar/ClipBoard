from bs4 import BeautifulSoup

# Read the HTML file
html_file_path = 'path_to_your_file.html'  # Replace with your HTML file path

with open(html_file_path, 'r', encoding='utf-8') as file:
    html_content = file.read()

# Parse the HTML content using BeautifulSoup
soup = BeautifulSoup(html_content, 'lxml')

# Find the input field with id="x"
input_field = soup.find('input', {'id': 'x'})

# Check if the input field is found and extract its value
if input_field:
    input_value = input_field.get('value')  # Extract the value attribute
    print(f"Value of input field with id='x': {input_value}")
else:
    print("Input field with id='x' not found.")